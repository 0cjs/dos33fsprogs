10 GR
20 FOR X=0 TO 39:FOR Y=0 TO 39
30 YY=Y:XX=(20-X)
80 C1=(YY-(XX*T/256))
85 C2=(YY*T/256+XX)
87 A=C1:B=C2:GOSUB 120
88 C=C-(INT(C/4)*4)
95 COLOR=C:PLOT X,Y
100 NEXT Y,X
110 END
120 C=0
140 FOR S =0 TO 7
150 A = INT(A)/2 : ABIT = ( A-INT(A) ) * 2
160 B = INT(B)/2 : BBIT = ( B-INT(B) ) * 2
170 IF ABIT + BBIT > 1 THEN C = C + 256
180 C = C / 2 : NEXT
190 RETURN

